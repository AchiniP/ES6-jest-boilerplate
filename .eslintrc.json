{
    "extends": "airbnb",
    "plugins": [
        "import",
        "jest"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "es6": true,
        "jest/globals": true
    }
}
